--RED LIGHT GREEN LIGHT STUFF

--RED LIGHT GREEN LIGHT VALUES
getgenv().autopull = false

--AUTOPULL
function pull()
    spawn(function()
        while wait() do
            if not getgenv().autopull then break end
            local args = {
                [1] = 0.99999999999999
            }
            
            game:GetService("ReplicatedStorage").Pull:FireServer(unpack(args))
        end
    end)
end


